<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.buct.coder.mapper.StudentSubmitLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.buct.coder.model.StudentSubmitLog">
        <id column="id" property="id" />
        <result column="stu_id" property="stuId" />
        <result column="submit_id" property="submitId" />
        <result column="submit_time" property="submitTime" />
        <result column="question_name" property="questionName" />
        <result column="language" property="language" />
        <result column="status" property="status" />
        <result column="code" property="code" />
        <result column="contest_id" property="contestId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, stu_id, submit_id, submit_time, question_name, language, status, code, contest_id
    </sql>

</mapper>
